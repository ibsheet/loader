{
  "name": "@ibsheet/loader",
  "jslib": {
    "filename": "ibsheet-loader",
    "global": "IBSheetLoader"
  },
  "version": "1.1.25",
  "description": "Dynamically load support module for IBSheet",
  "exports": {
    ".": {
      "browser": "./dist/umd/ibsheet-loader.min.js",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "run-s clean build:prod:* build:types postbuild:meta",
    "build:dev": "webpack --config ./webpack.dev.cjs --progress --color --stats-error-details",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.prod.cjs --progress --color --stats-error-details --bail",
    "prebuild:prod:esm": "trash dist/esm",
    "build:prod:esm": "cross-env BUILD_TARGET=esm run-s build:prod",
    "prebuild:prod:cjs": "trash dist/cjs",
    "build:prod:cjs": "cross-env BUILD_TARGET=cjs run-s build:prod",
    "prebuild:prod:umd": "trash dist/umd",
    "build:prod:umd": "cross-env BUILD_TARGET=umd run-s build:prod",
    "build:prod:umd__min": "cross-env BUILD_TARGET=umd.min run-s build:prod",
    "build:types": "tsc",
    "postbuild:meta": "node scripts/postbuild.cjs",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write --list-different",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "fix:eslint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "test": "run-s build:prod:esm test:*",
    "test:lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" && prettier \"src/**/*.ts\" --list-different",
    "cov": "run-s cov:html && open-cli coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && open-cli dist/docs/index.html",
    "predoc:html": "trash dist/docs",
    "doc:html": "typedoc src/ --options ./typedoc.js --out dist/docs",
    "doc:json": "typedoc src/ --options ./typedoc.js --json dist/docs/typedoc.json",
    "predoc:publish": "run-s doc:html",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d dist/docs",
    "clean": "node scripts/cleanDist.cjs && trash coverage .nyc_output test",
    "reset": "git clean -dfx && git reset --hard && yarn",
    "info": "npm-scripts-info",
    "serve": "run-s serve:dev",
    "serve:dev": "webpack-dev-server --mode development --config ./webpack.dev.cjs --hot --inline --progress --watch",
    "serve:prod": "cross-env NODE_ENV=production webpack-dev-server --config ./webpack.prod.cjs --port 3040 --host 0.0.0.0 --hot --inline --progress --watch --open --content-base dist/umd",
    "start": "run-s serve:dev",
    "prepare-release": "run-s reset test cov:check",
    "prerelease": "run-s test:*",
    "release": "standard-version",
    "release:patch": "yarn release --release-as patch",
    "release:minor": "yarn release --release-as minor"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "release": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibsheet/loader.git"
  },
  "author": "IB Leaders <support@ibleaders.co.kr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ibsheet/loader/issues"
  },
  "homepage": "https://github.com/ibsheet/loader#readme",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/is-url": "^1.2.28",
    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.138",
    "@types/shortid": "^0.0.29",
    "@types/uuid": "^3.4.13",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "ava": "^6.4.0",
    "babel-loader": "^10.0.0",
    "bannerjs": "^1.0.6",
    "codecov": "^3.6.1",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv": "^8.1.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-standard": "^5.0.0",
    "fs-extra": "^11.3.0",
    "generate-package-json-webpack-plugin": "^2.6.0",
    "gh-pages": "^6.3.0",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "npm-scripts-info": "^0.1.0",
    "nyc": "^15.1.0",
    "open-cli": "^8.0.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.6.2",
    "sass": "^1.65.1",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "standard": "^17.1.2",
    "standard-version": "^9.5.0",
    "string-replace-loader": "^3.2.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "trash-cli": "^6.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1",
    "webpack-replace-plugin": "^0.0.4"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "babel-plugin-istanbul": "^7.0.0",
    "core-js": "^3.43.0",
    "is-url": "^1.2.4",
    "shortid": "^2.2.15",
    "ts-loader": "^9.5.2",
    "url": "^0.11.4",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "serialize-javascript": "^6.0.2"
  },
  "sideEffects": false,
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "endOfLine": "auto",
    "singleQuote": true
  },
  "ava": {
    "require": [
      "ts-node/register"
    ],
    "files": [
      "src/**/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": false,
    "all": true,
    "instrument": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ]
  }
}
